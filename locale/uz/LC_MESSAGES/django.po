#: authentication/permissions.py:18
msgid "Your account is deactivated."
msgstr "Sizning profilingiz deaktivatsiya qilingan."

#: authentication/serializers.py:28
msgid "Email must be valid!"
msgstr "Email manzili yaroqli bo‘lishi shart!"

#: authentication/serializers.py:32
msgid "Email already registered!"
msgstr "Email manzil allaqachon ro‘yxatdan o'tgan!"

#: authentication/serializers.py:38 authentication/serializers.py:133
msgid "Username should contain only letters, numbers and underscores."
msgstr ""
"Foydalanuvchi nomi faqat harflar, raqamlar va pastki chiziqdan iborat "
"bo‘lishi shart."

#: authentication/serializers.py:48 authentication/serializers.py:143
msgid "Username must be at least 3 characters long."
msgstr "Foydalanuvchi nomi kamida 3 ta belgidan iborat bo‘lishi kerak."

#: authentication/serializers.py:51 authentication/serializers.py:147
msgid "This username is not valid!"
msgstr "Bu foydalanuvchi nomi yaroqsiz!"

#: authentication/serializers.py:54 authentication/serializers.py:156
msgid "This username is already taken!"
msgstr "Bu foydalanuvchi nomi allaqachon band!"

#: authentication/serializers.py:65
msgid ""
"Password must be 8–64 characters long and include at least one uppercase "
"letter, one lowercase letter, one number, and one special character."
msgstr ""
"Parol 8–64 ta belgidan iborat bo‘lishi va kamida bitta katta harf, bitta "
"kichik harf, bitta raqam hamda bitta maxsus belgini o‘z ichiga olishi kerak."

#: authentication/serializers.py:105
msgid "Code must be 6 digits."
msgstr "Kod 6 raqamdan iborat bo‘lishi kerak."

#: authentication/serializers.py:114
msgid "Invalid or expired code!"
msgstr "Kod yaroqsiz yoki amal qilish muddati tugagan!"

#: authentication/views.py:63
msgid "Verification code sent successfully"
msgstr "Tasdiqlash kodi muvaffaqiyatli yuborildi"

